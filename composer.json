{
    "name": "minjemin/minjemin-storage-adapter",
    "description": "Flysystem Adapter for Minjemin Storage",
    "version": "1.0",
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "mockery/mockery": "^1.3",
        "phpunit/php-code-coverage": "^8.0",
        "vlucas/phpdotenv": "^4.1",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Minjemin\\Flysystem\\MinjeminStorage\\": "src/",
            "Minjemin\\Flysystem\\MinjeminStorage\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alif Jafar",
            "email": "alifjafar@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "league/flysystem": "^1.0",
        "illuminate/support": "^5.5|^6.0|^7.0",
        "kitetail/zttp": "^0.6.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "tdd": "bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Minjemin\\Flysystem\\MinjeminStorage\\MinjeminStorageServiceProvider"
            ]
        }
    }
}
